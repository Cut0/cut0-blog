{
  "name": "cut0-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "node ./MarkdownToHtml.js && astro check && astro build",
    "preview": "astro preview",
    "dev:depricated": "next dev",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint --ext ts,tsx,js,astro ./src",
    "lint:prettier": "prettier --ignore-path .prettierignore  --check '**/*.{js,ts,tsx,css,astro}'",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "eslint --fix --ext ts,tsx,js,astro .",
    "format:prettier": "prettier --ignore-path .prettierignore  --write '**/*.{js,ts,tsx,css,astro}'"
  },
  "dependencies": {
    "astro": "^2.0.10",
    "dayjs": "^1.11.5",
    "microcms-js-sdk": "^2.0.0",
    "prismjs": "^1.29.0",
    "sanitize.css": "^13.0.0",
    "zenn-content-css": "0.1.141",
    "zenn-markdown-html": "0.1.141"
  },
  "devDependencies": {
    "@astrojs/partytown": "^1.0.3",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.52.0",
    "astro-compress": "^1.1.32",
    "dotenv": "^16.0.3",
    "eslint": "8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.23.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-astro": "^0.8.0",
    "typescript": "4.6.4"
  }
}
